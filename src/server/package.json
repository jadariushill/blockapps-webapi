{
  "name": "blockapps-webapi",
  "version": "1.0.2",
  "dependencies": {
    "commit-analyzer-fail-on-no-release": "^1.0.1",
    "express": "^4.18.2",
    "node-fetch": "^2.6.12"
  },
  "devDependencies": {
    "cypress": "^12.17.2",
    "semantic-release": "^21.0.7"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "commit-analyzer-fail-on-no-release",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ]
    ]
  }
}
